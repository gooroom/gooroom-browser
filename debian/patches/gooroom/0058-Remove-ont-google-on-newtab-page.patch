   afterBarScript.appendChild(document.createTextNode(ogb.afterBarScript));
-  ogElem.parentNode.insertBefore(afterBarScript, ogElem.nextSibling);
 
   $('one-google-end-of-body').innerHTML = ogb.endOfBodyHtml;
 
